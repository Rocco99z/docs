{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@simba/components": [
        "packages/index.tsx"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "**/src/**/*",
    "packages/index.tsx"
  ]
}